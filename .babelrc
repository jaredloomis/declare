{
    "presets": ["es2017", "es2016", "es2015", "react", "flow"],
    "plugins": [
        "transform-flow-strip-types",
        "transform-runtime",
        "transform-object-rest-spread",
        ["transform-async-to-module-method", {
            "module": "bluebird",
            "method": "coroutine"
        }]
    ]
}
