  "license": "All Rights Reserved",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "prisma generate && graphql-codegen --config codegen.ts && tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf .parcel-cache node_modules graphql.schema.json",
    "format": "prettier src --write",
    "migrate": "prisma generate && prisma db push && prisma migrate dev"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/selenium-webdriver": "^4.1.21",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@graphql-tools/schema": "^10.0.2",
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "declare-server-common": "workspace:*",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.3",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "selenium-webdriver": "^4.16.0",
    "ws": "^8.16.0"
  }
